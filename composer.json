{
    "name": "lunax-project/lunax",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "brainmaestro/composer-git-hooks": "^2.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "authors": [
        {
            "name": "William Cunha",
            "email": "williamoliveira2@hotmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {"Lunax\\": "src/Lunax/Core/"}
